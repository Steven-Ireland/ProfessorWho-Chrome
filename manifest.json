{
  "name": "WPI Who's My Professor",
  "description": "Highlight bannerweb professors based on ratemyprofessor rating",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [ {
    "js": [ "jquery-2.1.1.min.js", "background.js" ],
    "matches": [ "http://bannerweb.wpi.edu/*", "https://*/*"],
	"run-at": "document_idle"
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "http://commote.net/"
  ],
  
  "manifest_version": 2
}
